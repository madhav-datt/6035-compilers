#include <stdbool.h>
#define int long long
extern printf();
int get_int ( int x ) {
  return x;
}
bool get_bool ( bool x ) {
  return x;
}
int foo ( ) {
  int a, b, c, i;
  bool x;
  a = get_int ( 7 );
  b = get_int ( 2 );
  x = get_bool ( true );
